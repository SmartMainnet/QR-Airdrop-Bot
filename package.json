    "@grammyjs/i18n": "1.1.2",
    "@grammyjs/parse-mode": "1.10.0",
    "@grammyjs/ratelimiter": "^1.2.0",
    "@grammyjs/types": "3.14.0",
    "@hono/node-server": "1.13.2",
    "axios": "^1.7.7",
    "callback-data": "1.1.1",
    "grammy": "1.30.0",
    "grammy-guard": "0.5.0",
    "hono": "4.6.5",
    "iso-639-1": "3.1.3",
    "mongoose": "^8.7.2",
    "pino": "9.5.0",
    "pino-pretty": "11.3.0",
    "prom-client": "^15.1.3",
    "telegram-bot-template": "file:",

{
  "name": "telegram-bot-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "imports": {
    "#root/*": "./build/src/*"
  },
  "author": "SmartMainnet",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsc --noEmit false",
    "dev": "tsc-watch --onSuccess \"tsx ./src/main.ts\"",
